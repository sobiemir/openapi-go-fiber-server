package {{modelPackage}}
{{#each models}}
	{{~>misc/generic_imports bare=1 enl=1}}

	{{~#with model}}
		{{~>misc/nl n=1}}
		{{~>misc/description tab=0 enl=1}}

		{{~#if isEnum}}
			{{~>model/enum}}
		{{~else}}
			{{~>model/struct}}

func (mdl *{{classname}}) setDefaultValues() {
			{{~#each vars}}
				{{~#if defaultValue}}
	mdl.{{name}} = {{{defaultValue}}}
				{{~/if~}}
			{{~/each}}
}
		{{~/if}}
	{{~/with}}
{{~/each}}
